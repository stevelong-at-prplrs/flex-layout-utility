{
  "name": "rgs-interactive-layout",
  "version": "1.0.0",
  "description": "An interactive utility for using react grid system layouts in UIs.",
  "browser": "dist/index.html",
  "scripts": {
    "serve": "serve dist",
    "webpack-watch": "webpack --config webpack-config.js",
    "bs": "gulp browser-sync",
    "dev": "concurrently \"npm run bs\" \"npm run webpack-watch\"",
    "publish": "rm -rf ./docs && cp -rf ./dist ./docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stevelong-at-prplrs/rgs-interactive-layout.git"
  },
  "author": "Steve Long",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/stevelong-at-prplrs/rgs-interactive-layout/issues"
  },
  "homepage": "https://github.com/stevelong-at-prplrs/rgs-interactive-layout#readme",
  "dependencies": {
    "concurrently": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-grid-system": "^7.1.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-test-renderer": "^16.9.2",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "browser-sync": "^2.26.14",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gulp": "^4.0.2",
    "jest": "^26.0.1",
    "react-test-renderer": "^16.13.1",
    "serve": "^11.3.2",
    "ts-loader": "^6.2.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
